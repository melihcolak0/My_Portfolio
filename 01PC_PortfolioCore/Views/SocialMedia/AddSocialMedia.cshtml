@model SocialMedia
@{
    ViewData["Title"] = "AddSocialMedia";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}
<h4>Yeni Sosyal Medya Hesabı Ekleme</h4>
<br />
<form class="form-group" method="post">
    <label>Sosyal Medya</label>
    <input type="text" class="form-control" asp-for="Name" style="border:1px solid black" />
    <br />
    <label>Url</label>
    <input type="text" class="form-control" asp-for="Url" style="border:1px solid black" />
    <br />
    <label>İkon</label>
    <input type="text" class="form-control" asp-for="Icon" style="border:1px solid black" />
    <br />
    <button class="btn btn-primary">Ekle</button>
</form>
